<!-- Product Details -->
<div class="product-details-area w-full px-[100px] pt-[40px] flex gap-[40px]">
    <!-- product-details-left image -->
    <div class="product-details-left w-7/12 flex gap-[14px]">
        <div class="slider-nav product-image-sidebar flex flex-col gap-[14px]">
            <div class="product-image1 w-[152px] h-[167px]">
                <img src="{{ 'productImg1.png' | asset_url }}" class="" />
            </div>
            <div class="product-image2 w-[152px] h-[167px]">
                <img src="{{ 'productImg2.png' | asset_url }}" class="" />
            </div>
            <div class="product-image3 w-[152px] h-[167px]">
                <img src="{{ 'productImg3.png' | asset_url }}" class="" />
            </div>
        </div>
        <div class="slider-for product-main-image w-[444px] h-[530px]">
              <div>
                  <img src="{{ product.featured_image | image_url: width: 300 }}" alt="{{ item.title }}" class="w-full h-[530px] object-contain" />
            </div>
    <div><img src="{{ 'productImg2.png' | asset_url }}" class="w-full h-[530px] object-contain" /></div>
    <div><img src="{{ 'productImg3.png' | asset_url }}" class="w-full h-[530px] object-contain" /></div>
        </div>
    </div>

    <!-- product details right -->
    <div class="product-details-right w-5/12 flex flex-col gap-[12px] pt-[5px] pb-[25px]">

        <!-- product text -->
        <div class="text-area-wrapper w-full flex flex-col gap-[15.30px]">
            <h1 class="text-black font-[700] min-w-[610px] text-[39.25px] not-italic leading-none font-integralcf">
                {{ product.title }}
                </h1>

            <!-- rating -->
            <div class="rating flex gap-[14px] items-center pt-[2px]">
                <div class="rating-icon flex gap-[7.10px]">
                    <svg xmlns="http://www.w3.org/2000/svg" width="25" height="23" viewBox="0 0 25 23" fill="none">
                        <path
                            d="M12.3562 0L15.8569 7.53796L24.1077 8.53794L18.0204 14.1966L19.619 22.3526L12.3562 18.3119L5.09341 22.3526L6.69201 14.1966L0.604756 8.53794L8.85555 7.53796L12.3562 0Z"
                            fill="#FFC633" />
                    </svg>
                    <svg xmlns="http://www.w3.org/2000/svg" width="25" height="23" viewBox="0 0 25 23" fill="none">
                        <path
                            d="M12.3562 0L15.8569 7.53796L24.1077 8.53794L18.0204 14.1966L19.619 22.3526L12.3562 18.3119L5.09341 22.3526L6.69201 14.1966L0.604756 8.53794L8.85555 7.53796L12.3562 0Z"
                            fill="#FFC633" />
                    </svg>
                    <svg xmlns="http://www.w3.org/2000/svg" width="25" height="23" viewBox="0 0 25 23" fill="none">
                        <path
                            d="M12.3562 0L15.8569 7.53796L24.1077 8.53794L18.0204 14.1966L19.619 22.3526L12.3562 18.3119L5.09341 22.3526L6.69201 14.1966L0.604756 8.53794L8.85555 7.53796L12.3562 0Z"
                            fill="#FFC633" />
                    </svg>
                    <svg xmlns="http://www.w3.org/2000/svg" width="25" height="23" viewBox="0 0 25 23" fill="none">
                        <path
                            d="M12.3562 0L15.8569 7.53796L24.1077 8.53794L18.0204 14.1966L19.619 22.3526L12.3562 18.3119L5.09341 22.3526L6.69201 14.1966L0.604756 8.53794L8.85555 7.53796L12.3562 0Z"
                            fill="#FFC633" />
                    </svg>
                    <svg xmlns="http://www.w3.org/2000/svg" width="12" height="23" viewBox="0 0 12 23" fill="none">
                        <path
                            d="M4.73719 22.3526L12 18.3119V0L8.49932 7.53796L0.248535 8.53793L6.33579 14.1966L4.73719 22.3526Z"
                            fill="#FFC633" />
                    </svg>
                </div>
                <!-- rating rate -->
                <div class="rating-rate">
                    <h1 class="text-black font-satoshi text-[16px] not-italic font-normal leading-none">4.5/5</h1>
                </div>
            </div>

            <!-- product price -->
            <div class="product-price pt-[7.60px] pb-[2px] flex gap-[13.50px]">
                <h1 class="text-black font-satoshi text-[32px] not-italic font-extrabold leading-none">{{ product.price | money }}
                </h1>
                <h2
                    class="text-[rgba(0,0,0,0.3)] font-satoshi text-[32px] not-italic font-bold leading-none line-through">
                    {{ product.compare_at_price | money }}
                    </h2>
                <div
                    class="inline-flex px-[14px] py-[6px] justify-center items-center gap-[12px] rounded-[62px] bg-[rgba(255,51,51,0.1)]">
                    {% if product.compare_at_price > product.price  %}
                    {% assign discount = product.compare_at_price | minus: product.price %}
                     {% assign discount_percent = discount | times: 100.0 | divided_by: product.compare_at_price | round %}

                    <h1 class="text-[#F33] font-[Satoshi] text-[16px] not-italic font-medium leading-none"> 
                        - {{ discount_percent }}%
                    </h1>
                    {% endif %}
                </div>
            </div>

            <!-- text -->
            <p class="text-[rgba(0,0,0,0.6)] pb-[3px] font-satoshi text-[16px] not-italic font-normal leading-[22px]">
                {{ product.description}}
                </p>
            <!-- divider -->
            <div class="w-full h-[2px] bg-[rgba(0,0,0,0.10)]"></div>
        </div>


        <!-- Color -->
        <div class="text-area-wrapper w-full pt-[4px] flex flex-col gap-[20.50px]">
            <!-- Select Color -->
            <div class="select-color flex flex-col gap-[12px]">
                <h1 class="text-[rgba(0,0,0,0.6)]  font-satoshi text-[16px] not-italic font-normal leading-normal">
                    Select Colors</h1>
                <div class="color flex gap-[16px]">
                    <button
                        class="w-[37px] h-[37px] rounded-full bg-[#4F4631] flex flex-shrink-0 border-none justify-center items-center">
                        <svg xmlns="http://www.w3.org/2000/svg" class="w-4 h-4 fill-[#FFFFFF]" viewBox="0 0 16 16"
